{
  "name": "app-engine-trebuchet",
  "productName": "App Engine Trebuchet",
  "version": "0.0.1",
  "description": "App Engine Trebuchet is a visual tool that makes it easier to create and deploy applications for App Engine.  It's meant to be a replacement for the App Engine Launcher.",
  "main": "main.js",
  "scripts": {
    "start": "electron . --log-level=silly",
    "build": "webpack",
    "dev": "webpack --watch",
    "package": "electron-packager . 'App Engine Trebuchet' --out bin --icon build/icon.icns --overwrite",
    "pack": "build --dir",
    "dist": "build -m",
    "logs-local": "cat ~/Library/Logs/App\\ Engine\\ Trebuchet/log.log",
    "open-local": "open /Applications/App\\ Engine\\ Trebuchet.app/ --args --log-level silly"
  },
  "build": {
    "appId": "app-engine-trebuchet",
    "extraResources": "templates/",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {}
  },
  "postinstall": "install-app-deps",
  "repository": "https://github.com/JustinBeckwith/trebuchet",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Justin Beckwith <justin.beckwith@gmail.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "electron": "^1.4.1",
    "electron-builder": "^10.12.0",
    "electron-connect": "^0.6.1",
    "electron-connect-webpack-plugin": "^0.1.1",
    "electron-packager": "^8.4.0",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "always-tail": "^0.2.0",
    "babel-polyfill": "^6.20.0",
    "chmodr": "^1.0.2",
    "electron-log": "^1.2.2",
    "fix-path": "^2.1.0",
    "fs-extra": "^1.0.0",
    "js-yaml": "^3.7.0",
    "localforage": "^1.4.3",
    "lodash": "^4.17.2",
    "material-ui": "^0.16.5",
    "project-name-generator": "^2.1.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-tap-event-plugin": "^2.0.1",
    "spawn-sync": "^1.0.15",
    "uuid": "^3.0.1"
  }
}
